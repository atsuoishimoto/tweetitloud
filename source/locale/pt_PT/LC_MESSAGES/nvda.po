#: txt2tags.py:211
msgid "Usage: %s [OPTIONS] [infile.t2t ...]"
msgstr "Utilização: %s [OPÇÕES] [ficheiro.t2t ...]"

#: txt2tags.py:213
msgid "  -t, --target=TYPE   set target document type. currently supported:"
msgstr "  -t, --target=TIPO   define o formato do destino. tipos suportados:"

#: txt2tags.py:216
msgid "  -i, --infile=FILE   set FILE as the input file name ('-' for STDIN)"
msgstr "  -i, --infile=FICHEIRO    define FICHEIRO como o ficheiro fonte ('-' para STDIN)"

#: txt2tags.py:217
msgid "  -o, --outfile=FILE  set FILE as the output file name ('-' for STDOUT)"
msgstr "  -o, --outfile=FICHEIRO   define FICHEIRO como o ficheiro de destino ('-' para STDOUT)"

#: txt2tags.py:218
msgid "  -H, --no-headers    suppress header, title and footer contents"
msgstr "  -H, --no-headers    remove os cabeçalhos, título e rodapé"

#: txt2tags.py:219
msgid "      --headers       show header, title and footer contents (default ON)"
msgstr "      --headers       mostra os cabeçalhos, título e rodapé (ACTIVADO por predefinição)"

#: txt2tags.py:220
msgid "      --encoding=ENC  set target file encoding (utf-8, iso-8859-1, etc)"
msgstr "      --encoding=ENC  definir a codificação de destino (utf-8, iso-8859-1, etc)"

#: txt2tags.py:221
msgid "      --style=FILE    use FILE as the document style (like HTML CSS)"
msgstr "      --style=FICHEIRO     utiliza FICHEIRO como estilo do documento (CSS no Html)"

#: txt2tags.py:222
msgid "      --css-sugar     insert CSS-friendly tags for HTML and XHTML targets"
msgstr "      --css-sugar     introduz tags compatíveis com CSS nos destinos HTML e XHTML"

#: txt2tags.py:223
msgid "      --css-inside    insert CSS file contents inside HTML/XHTML headers"
msgstr "      --css-inside    introduz o conteúdo do ficheiro CSS no documento HTML/XHTML"

#: txt2tags.py:224
msgid "      --mask-email    hide email from spam robots. x@y.z turns <x (a) y z>"
msgstr "      --mask-email    esconde e-mails dos robôs de SPAM. x@y.z vira <x (a) y z>"

#: txt2tags.py:225
msgid "      --toc           add TOC (Table of Contents) to target document"
msgstr "      --toc           inclui o Índice (Table of Contents) no documento de destino"

#: txt2tags.py:226
msgid "      --toc-only      print document TOC and exit"
msgstr "      --toc-only      mostra o Índice do documento e sai"

#: txt2tags.py:227
msgid "      --toc-level=N   set maximum TOC level (depth) to N"
msgstr "      --toc-level=N   define o nível máximo do Índice para N"

#: txt2tags.py:228
msgid "  -n, --enum-title    enumerate all titles as 1, 1.1, 1.1.1, etc"
msgstr "  -n, --enum-title    Numerar todos os títulos (1, 1.1, 1.1.1, etc)"

#: txt2tags.py:229
msgid "  -C, --config-file=F read config from file F"
msgstr "  -C, --config-file=F carrega o ficheiro de configuração F"

#: txt2tags.py:230
msgid "      --rc            read user config file ~/.txt2tagsrc (default ON)"
msgstr "      --rc            lê a config do utilizador em ~/.txt2tagsrc (LIGADO por pré-definição)"

#: txt2tags.py:231
msgid "      --gui           invoke Graphical Tk Interface"
msgstr "      --gui           carrega a interface gráfica escrita em TK"

#: txt2tags.py:232
msgid "  -q, --quiet         quiet mode, suppress all output (except errors)"
msgstr "  -q, --quiet         modo silencioso, sem mensagens (excepto erros)"

#: txt2tags.py:233
msgid "  -v, --verbose       print informative messages during conversion"
msgstr "  -v, --verbose       mostra mensagens de informação durante a conversão"

#: txt2tags.py:234
msgid "  -h, --help          print this help information and exit"
msgstr "  -h, --ajuda          mostra esta informação de ajuda e sai"

#: txt2tags.py:235
msgid "  -V, --version       print program version and exit"
msgstr "  -V, --version       mostra a versão do programa e sai"

#: txt2tags.py:236
msgid "      --dump-config   print all the config found and exit"
msgstr "      --dump-config   mostra todas as configurações encontradas e sai"

#: txt2tags.py:237
msgid "      --dump-source   print the document source, with includes expanded"
msgstr "      --dump-source   mostra os fontes com os includes expandidos"

#: txt2tags.py:239
msgid "Turn OFF options:"
msgstr "Desactiva as opções:"

