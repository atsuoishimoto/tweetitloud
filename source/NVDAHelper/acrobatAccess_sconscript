Import('env')

tlbFile,headerFile,iidSourceFile,proxySourceFile,dlldataSourceFile=env.TypeLibrary(source=['../include/acrobatAccess/acrobatAccess.idl'])
iidObjFile=env.Object(iidSourceFile)
proxyObjFile=env.Object(proxySourceFile)
dlldataObjFile=env.Object(dlldataSourceFile)

Return(['tlbFile','headerFile','iidObjFile','proxyObjFile','dlldataObjFile'])
