Import([
	'env',
	'remoteLib',
])

debugObj=env.Object("./debug","#common/debug.cpp")

vbufBaseStaticLib=env.StaticLibrary(
	target="vbufBase_static",
	source=[
		"storage.cpp",
		"utils.cpp",
		"lock.cpp",
		"backend.cpp",
		debugObj,
		remoteLib[1] if env['release'] else remoteLib[2],
	],
)

Return('vbufBaseStaticLib')
